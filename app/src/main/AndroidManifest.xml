<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.shubham.fit_treat">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Fit Treat"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.shubham.fit_treat.registerActivity"></activity>
        <activity android:name="com.shubham.fit_treat.loginActivity" />
        <activity android:name="com.shubham.fit_treat.BMR_BMI" />
        <activity android:name="com.shubham.fit_treat.StepCountActivity" />
        <activity android:name="com.shubham.fit_treat.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>